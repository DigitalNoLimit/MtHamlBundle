{
    "name": "mthaml/mthaml-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides a HAML templating engine",
    "keywords": ["HAML"],
    "homepage": "https://github.com/arnaud-lb/MtHamlBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Le Blanc",
            "email": "arnaud.lb@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "mthaml/mthaml": ">=1.1.0"
    },
    "require-dev": {
        "symfony/symfony": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*"
    },
    "autoload": {
        "psr-0": {
            "MtHamlBundle": ""
        }
    },
    "target-dir": "MtHamlBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
